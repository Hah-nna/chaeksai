<?xml version="1.0" encoding="utf-8"?><!--writer: 정지영-->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.myPage.PrivacyManagementFragment">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/fragment_privacy_management"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/privacy_management_fragment"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/privacy_management_to_followers_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Followers"
        app:layout_constraintTop_toBottomOf="@id/fragment_privacy_management"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/privacy_management_to_login_management_btn"
        app:layout_constraintHorizontal_chainStyle="spread"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="24dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/privacy_management_to_login_management_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="LoginManagement"
        app:layout_constraintTop_toBottomOf="@id/fragment_privacy_management"
        app:layout_constraintStart_toEndOf="@id/privacy_management_to_followers_btn"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp" />

</androidx.constraintlayout.widget.ConstraintLayout>